{
  "framework": "nextjs",
  "buildCommand": "next build ./app",
  "outputDirectory": "./app/.next",
  "functions": {
    "app/pages/api/upload.js": {
      "maxDuration": 30
    },
    "app/pages/api/deploy.js": {
      "maxDuration": 30
    },
    "app/pages/api/publish.js": {
      "maxDuration": 20
    }
  },
  "rewrites": [
    {
      "source": "/:ens(.*\\.eth)/:path*",
      "destination": "/api/proxy/:ens/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/proxy/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    }
  ]
}